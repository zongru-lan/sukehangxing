<Window x:Class="UI.XRay.Security.Scanner.RenderEngine22"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:renderEngine="clr-namespace:UI.XRay.RenderEngine;assembly=UI.XRay.RenderEngine"
        xmlns:local="clr-namespace:UI.XRay.Security.Scanner"
        mc:Ignorable="d"
        WindowStyle="ToolWindow"
        Title="图像查看处理器" Height="580" Width="1600">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="79*"/>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="1069*"/>
        </Grid.ColumnDefinitions>
        <Button Name="Inverse" HorizontalAlignment="Left" Height="41" Margin="10,490,0,0" VerticalAlignment="Top" Width="50" Click="Inverse_Click">
            <Button.Content>
                <Image Source="Icons/Inverse.png"  Focusable="True" Width="31" Height="36"/>
            </Button.Content>
        </Button >
        <renderEngine:ImageControlWpf Name="ImageControl" Margin="0,0,0,103" Grid.ColumnSpan="3"/>
        <Button Name="Sen" HorizontalAlignment="Left" Margin="71,490,0,0" VerticalAlignment="Top" Height="41" Width="50" Click="SuperEnhance_Click" Grid.ColumnSpan="2">
            <Button.Content>
                <Image Source="Icons/Sen.png" />
            </Button.Content>
        </Button >
        <Button Name="Recover" HorizontalAlignment="Left" Height="41" Margin="52,490,0,0" VerticalAlignment="Top" Width="50" Click="Recover_Click" Grid.Column="1" Grid.ColumnSpan="2">
            <Image Source="Icons/Recover.png" />
        </Button >
        <Button Name="BW" HorizontalAlignment="Left" Height="41" Margin="62,490,0,0" VerticalAlignment="Top" Width="50" Click="BW_Click" Grid.Column="2">
            <Image Source="Icons/BW.png" />
        </Button >
        <Button Name="HAHA" HorizontalAlignment="Left" Height="41" Margin="125,490,0,0" VerticalAlignment="Top" Width="50" Click="HAHA_Click" Grid.Column="2">
            <Image Source="Icons/HAHA.png" />
        </Button >
        <Button Name="XIXI" HorizontalAlignment="Left" Height="41" Margin="187,490,0,0" VerticalAlignment="Top" Width="50" Click="XIXI_Click" Grid.Column="2">
            <Image Source="Icons/XIXI.png" />
        </Button >
        <Button Name="HI" HorizontalAlignment="Left" Height="41" Margin="254,490,0,0" VerticalAlignment="Top" Width="50" Click="HI_Click" Grid.Column="2">
            <Image Source="Icons/HI.png" />
        </Button >
        <Button Name="Organic" HorizontalAlignment="Left" Height="41" Margin="328,490,0,0" VerticalAlignment="Top" Width="50" Click="Organic_Click" Grid.Column="2">
            <Image Source="Icons/Organic.png" />
        </Button >
        <Button Name="NonOrganic" HorizontalAlignment="Left" Height="41" Margin="393,490,0,0" VerticalAlignment="Top" Width="50" Click="NonOrganic_Click" Grid.Column="2">
            <Image Source="Icons/Nonorganinc.png" />
        </Button >
        <Button Name="ZZ" HorizontalAlignment="Left" Height="41" Margin="459,490,0,0" VerticalAlignment="Top" Width="50" Click="ZZ_Click" Grid.Column="2">
            <Image Source="Icons/789.png" />
        </Button >
    </Grid>
</Window>
